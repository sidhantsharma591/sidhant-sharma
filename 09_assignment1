a=[[1,2],[3,4]]
rows=len(a)
col=len(a(0))

if rows!=col :
print("matrix is not a square matrix")

else:
if rows==2:
det=a[0][0]*a[1][1]-a[1][0]*a[0][1]
print("det:",det)


else if rows==3:

det=(a[0][0]*(a[1][1]*a[2][2]-a[1][2]*a[2][1])) - (a[0][1](a[1][0]*a[2][2]-a[1][2]*a[2][0])) + (a[0][2](a[1][0]*a[2][1]-a[1][1]*a[2][0]))
print("det:",det)

else:
print("only square matrices are suppoeted")
